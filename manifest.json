{
    "manifest_version": 3,
    "name": "Simulated Input",
    "version": "1.0",
    "description": "将输入的字符串分发到所配置的元素上。",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icons/icon128.png"
        }
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "icons": {
        "128": "icons/icon128.png"
    }
}